  "scripts": {
    "test": "jest --verbose --silent ./test",
    "watch:dev:test": "jest --verbose --silent --watchAll ./test",
    "build:dev:client": "rm -rf dist/* && webpack --mode development --config webpack.client.config.js",
    "build:dev:ssr-server": "npm run build:dev:client && webpack --mode development --config webpack.server.config.js",
    "build:production:client": "rm -rf dist/* && cross-env NODE_ENV=production webpack --mode production --config webpack.client.config.js",
    "build:production:ssr-server": "npm run build:production:client && cross-env NODE_ENV=production webpack --mode production --config webpack.server.config.js",
    "build:production:*": "npm run build:production:ssr-server",
    "start:dev:client-webpack-dev-server": "rm -rf dist/* && cross-env NODE_ENV=development SSR=false SW_PREFETCH=false APP_API_BASE='' webpack-dev-server --open --mode development --config webpack.client.config.js",
    "start:dev:ssr-server": "cross-env NODE_ENV=development SSR=true APP_API_BASE='http://127.0.0.1:10000' node -r source-map-support/register dist/server",
    "start:dev:mock-api-server": "node src/server/mockApi"
  },
